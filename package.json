{
  "name": "kevin",
  "version": "1.0.0",
  "description": "My Online Resume",
  "main": "index.js",
  "scripts": {
    "start": "lite-server",
    "build-css": "sass src/scss:dist/css",
    "build-js": "babel src/js --out-dir dist/js --presets=@babel/preset-env",
    "watch-css": "sass --watch src/scss:dist/css",
    "watch-js": "babel src/js --out-dir dist/js --presets=@babel/preset-env --watch",
    "dev": "concurrently \"npm run watch-css\" \"npm run watch-js\" \"npm start\""
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "resume",
    "kevin"
  ],
  "author": "Mark Kevin Caccam",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "lite-server": "^2.6.1",
    "sass": "^1.78.0"
  }
}
